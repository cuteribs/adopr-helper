{
  "name": "adopr-helper",
  "displayName": "Azure Devops PullRequest Helper",
  "description": "Azure Devops PullRequest Helper",
  "version": "0.0.2",
  "publisher": "Cuteribs",
  "engines": {
    "vscode": "^1.102.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "adoHelper",
          "title": "Azure DevOps Helper",
          "icon": "./icon128.png"
        }
      ]
    },
    "views": {
      "adoHelper": [
        {
          "id": "adopr-helper.view",
          "name": "Azure DevOps Helper",
          "icon": "./icon128.png"
        }
      ]
    },
    "commands": [
      {
        "command": "adopr-helper.downloadChangedFiles",
        "title": "adopr: Download Changed Files from Azure DevOps PR",
        "icon": "$(download)"
      },
      {
        "command": "adopr-helper.setPAT",
        "title": "adopr: Set Personal Access Token (PAT)",
        "icon": "$(key)"
      },
      {
        "command": "adopr-helper.setOrg",
        "title": "adopr: Set Organization",
        "icon": "$(organization)"
      },
      {
        "command": "adopr-helper.setProject",
        "title": "adopr: Select Project",
        "icon": "$(project)"
      },
      {
        "command": "adopr-helper.showSelections",
        "title": "adopr: Show Selections",
        "icon": "$(info)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "adopr-helper.setPAT",
          "when": "view == adopr-helper.view",
          "group": "navigation"
        },
        {
          "command": "adopr-helper.setOrg",
          "when": "view == adopr-helper.view",
          "group": "navigation"
        },
        {
          "command": "adopr-helper.setProject",
          "when": "view == adopr-helper.view",
          "group": "navigation"
        },
        {
          "command": "adopr-helper.showSelections",
          "when": "view == adopr-helper.view",
          "group": "navigation"
        }
      ]
    },
    "configuration": {
      "title": "Azure Devops PullRequest Helper",
      "properties": {
        "adopr-helper.pat": {
          "type": "string",
          "default": "",
          "description": "Azure DevOps Personal Access Token (PAT) for authentication.",
          "scope": "application"
        },
        "adopr-helper.org": {
          "type": "string",
          "default": "",
          "description": "Azure DevOps Organization name.",
          "scope": "application"
        },
        "adopr-helper.project": {
          "type": "object",
          "default": null,
          "description": "Currently selected Azure DevOps project.",
          "scope": "workspace"
        },
        "adopr-helper.repo": {
          "type": "object",
          "default": null,
          "description": "Currently selected Azure DevOps repository.",
          "scope": "workspace"
        }
      }
    }
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/node-fetch": "2.6.12",
    "@types/vscode": "^1.102.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "9.31.0",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "diff": "8.0.2",
    "node-fetch": "2.7.0"
  }
}